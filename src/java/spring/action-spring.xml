<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<!-- ===================================Action配置========================================== -->

	 <bean name="LoginAction" class="com.action.LoginAction" scope="prototype"  >
    	<property name="loginService" ref="LoginService" />
    	<property name="syslogService" ref="SyslogService" />
    	<property name="rightService" ref="RightService" />
    	<property name="rulerService" ref="RulerService" />
    	<property name="userService" ref="UserService" />
	 </bean>  
	
     <bean name="RoleAction" class="com.action.RoleAction" scope="prototype">
	 	<property name="roleService" ref="RoleService" />
    	<property name="rulerService" ref="RulerService" />
	 	<property name="rightService" ref="RightService" />
	 	<property name="syslogService" ref="SyslogService" />
	 </bean>
	 
	 <bean name="RulerAction" class="com.action.xtgl.RulerAction" scope="prototype" >
    	<property name="rulerService" ref="RulerService" />
	 	<property name="rightService" ref="RightService" />
	 	<property name="syslogService" ref="SyslogService" />
	 </bean>
      
     <bean name="TestAction" class="com.action.TestAction" scope="prototype" >
    	<property name="testService" ref="TestService" />
	 	<property name="rightService" ref="RightService" />
	 	<property name="syslogService" ref="SyslogService" />
	 </bean>  
	 
	 <!-- 部门管理  唐梦岚-->
	<bean name="DeptAction" class="com.action.DeptAction" scope="prototype" >
    	<property name="deptService" ref="DeptService" />
    	<property name="rulerService" ref="RulerService" />
	 	<property name="rightService" ref="RightService" />
	 	<property name="syslogService" ref="SyslogService" />
	 </bean> 
	 
	 <bean name="UserAction" class="com.action.UserAction" scope="prototype">
	 	<property name="userService" ref="UserService"></property>
	 	<property name="rulerService" ref="RulerService"></property>
	 	<property name="fileService" ref="FileService"></property>
	 	<property name="rightService" ref="RightService" />
	 	<property name="syslogService" ref="SyslogService" />
	 </bean>
	 
	  <bean name="FileAction" class="com.action.FileAction" scope="prototype">
	 	<property name="fileService" ref="FileService"></property>
    	<property name="rulerService" ref="RulerService" />
	 	<property name="rightService" ref="RightService" />
	 	<property name="syslogService" ref="SyslogService" />
	 </bean>
	 
	  <bean name="UploadAction" class="com.action.UploadAction" scope="prototype">
	 	<property name="fileService" ref="FileService" />
    	<property name="rulerService" ref="RulerService" />
	 	<property name="rightService" ref="RightService" />
	 	<property name="syslogService" ref="SyslogService" />
	 </bean>
	 
	 <bean name="VehicleAction" class="com.action.VehicleAction" scope="prototype">
	 	<property name="rightService" ref="RightService" />
    	<property name="rulerService" ref="RulerService" />
	 	<property name="syslogService" ref="SyslogService" />
	 </bean>
	 
	 <bean name="ArticleAction" class="com.action.ArticleAction" scope="prototype" >
	 	<property name="articleService" ref="ArticleService" />
	 	<property name="categoryService" ref="CategoryService" />
    	<property name="rulerService" ref="RulerService" />
	 	<property name="rightService" ref="RightService" />
	 	<property name="syslogService" ref="SyslogService" />
	 </bean>
	 
	 <bean name="CategoryAction" class="com.action.CategoryAction" scope="prototype">
	 	<property name="categoryService" ref="CategoryService" />
    	<property name="rulerService" ref="RulerService" />
	 	<property name="rightService" ref="RightService" />
	 	<property name="syslogService" ref="SyslogService" />
	 </bean>
	 
	 <bean name="CkeditorUpload" class="com.action.CkeditorUpload" scope="prototype">
	 	<property name="fileService" ref="FileService"></property>
    	<property name="rulerService" ref="RulerService" />
	 	<property name="rightService" ref="RightService" />
	 	<property name="syslogService" ref="SyslogService" />
	 </bean>

	 <bean name="SyslogAction" class="com.action.SyslogAction" scope="prototype">
	 	<property name="syslogService" ref="SyslogService" />
    	<property name="rulerService" ref="RulerService" />
	 </bean>
	 
	 <bean name="FinanceAction" class="com.action.FinanceAction" scope="prototype">
	 	<property name="articleService" ref="ArticleService" />
	 	<property name="categoryService" ref="CategoryService" />
	 	<property name="rightService" ref="RightService" />
	 	<property name="syslogService" ref="SyslogService" />
    	<property name="rulerService" ref="RulerService" />
	 </bean>
	 
	 
	 
	 
</beans>